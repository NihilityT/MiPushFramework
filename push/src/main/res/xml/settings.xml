<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/settings_service_setting">

        <Preference
            android:key="activity_push_advance_setting"
            android:summary="@string/settings_summary_service_advance_setting"
            android:title="@string/settings_service_advance_setting">
            <intent
                android:targetClass="com.xiaomi.xmsf.ManageSpaceActivity"
                android:targetPackage="com.xiaomi.xmsf" />
        </Preference>

        <Preference
            android:key="configuration_directory"
            android:title="@string/settings_configuration_directory" />

        <Preference
            android:key="custom_notify_icon_path"
            android:title="Custom Notify Icon Path" />

    </PreferenceCategory>

    <PreferenceCategory android:title="@string/settings_debug">

        <Preference
            android:key="key_get_log"
            android:summary="@string/settings_get_log_summary"
            android:title="@string/settings_get_log" />

        <Preference android:title="@string/helplib_title">
            <intent
                android:targetClass="top.trumeet.mipushframework.help.HelpActivity"
                android:targetPackage="com.xiaomi.xmsf" />
        </Preference>
    </PreferenceCategory>
</PreferenceScreen>